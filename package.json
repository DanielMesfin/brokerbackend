{
  "name": "sms-backend",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --prefer-ts-exts src/main.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
